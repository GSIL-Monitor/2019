	class Rec{
	    constructor(width,height){
	        this.width = width;
	        this.height = height;
	    }
	
	    getArea(){
	        return this.width*this.height;
	    }
	
	}
	
	class Square extends Rec{
	    constructor(width,height){
	        super(width,height);
	    }
	
	}
	
	let square = new Square(100,100);
	console.log(square.getArea()); //10000



	let methodName ='sayName';
	let propertyName = 'name';
	
	class PersonClass{
	    constructor(name){
	        this.name = name;
	    }
	    get [propertyName](){
	        return name;
	    }
	    set [propertyName](value){
	        name = value;
	    }
	    [methodName](){
	        return console.log(this.name);
	    }
	}
	let person = new PersonClass('hello world');
	person.sayName(); //hello world
	console.log(person.name); //hello world
	





	ä¿è¯ä¸€ä¸ªç±»ä»…æœ‰ä¸€ä¸ªå®ä¾‹ ä¿®æ”¹è¯¥å®ä¾‹ï¼Œ ä¼šå½±å“åˆ°å…¶å®ƒçš„å®ä¾‹å¼•ç”¨ã€‚
	
		class Singleton {
		    constructor(data) {
		        if (Singleton.prototype.Instance === undefined) {
		            this.data = data;
		            Singleton.prototype.Instance = this;
		        }
		        return Singleton.prototype.Instance;
		    }
		}
		
		let ob1 = new Singleton("one");
		let ob2 = new Singleton("two");
		let ob3 = new Singleton("Three");
		
		
		console.log(ob1 === ob2);  // => true
		console.log(ob1 === ob3);  // => true
		
		ob3.init = 'Object flg';
		console.log(ob1);
		/* => 
		{
		  data:"one",
		  init:"Object flg",
		  __proto__:Object
		}
		*/
		
	




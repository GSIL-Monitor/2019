	es5 å®ç°è¿­ä»£å™¨
		function createiterator(items){
			var i = 0;
			return {
				next(){
					var done = (i >= items.length);
					var value = !done ? items[i++] : undefined;
					return{
						value,
						done
					}
				}
			}
		}
		
		const a = createiterator([1,2,3])
		console.log(a.next()); //{value: 1, done: false}
	
	
	forå¾ªç¯ä¸­ä½¿ç”¨è¿­ä»£å™¨
	
		function *createiterator(items){  
			for (let i=0; i<items.length; i++){
				yield items[i];
			}
		}
		const a = createiterator([1,2,3]);
		a.next()
		
	
	

class A{
	constructor(name){
		this.name = name;
	}
	static create(name){
		return new A(name)
	}
}
let a = A.create("sea")
console.log(a.name)

let t = new A() //é™æ€æ–¹æ³•ä¸èƒ½å¤åˆ¶ç»™t
console.log(t.create("sea")) // æŠ¥é”™


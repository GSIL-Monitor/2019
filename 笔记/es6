		let A_mixin = {
			A() {
				return json.stringify(this);
			}
		};
		
		let B_mixin = {
			B() {
				return this.length * this.width;
			}
		};
		
		function mixin(...mixins){
			var base = function(){};
			Object.assign(base.prototype, ...mixins)
			return base;
		}
		class square extends mixin(A_mixin, B_mixin){
			constructor(length) {
				super();
				this.length = length;
				this.width = length;
			}
		}
		var x = new square(3);
		console.log(x.B()); // 9
		console.log(x.A()); // "{"length":3,"width":3}
		
	

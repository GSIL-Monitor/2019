.search-input {
  margin: 20rpx 20rpx 30rpx;
}

.search-module {
  padding: 0 25rpx;
}

.search-module .title {
  color: #999999;
}

.search-module .right {
  color: #666;
}

.search-module .right image {
  width: 26rpx;
  height: 32rpx;
  margin-right: 10rpx;
}
 
.search-module-content {
  margin: 34rpx -9rpx 0;
}

.search-module-content navigator {
  background: #F0F0F0;
  border-radius:5rpx;
  padding: 11rpx 22rpx;
  display: inline-block; 
  margin: 0 9rpx 20rpx;
}

.search-margin-top {
  margin-top: 50rpx;
}

